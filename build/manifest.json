{
  "name": "Chrome插件Demo",
  "version": "1.0",
  "description": "React开发chrome插件Demo。",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2,
  "page_action": {
    "default_icon": "images/icon.png",
    "default_title": "React CRX",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/content.css"],
      "js": ["js/content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "browsingData",
    "<all_urls>"
  ],
  "web_accessible_resources": ["insert.js"]
}
