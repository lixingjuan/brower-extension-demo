{
  "name": "拦截请求",
  "description": "拦截chrome发出的请求，并自定义两个字段b-cookie 和 b-domain",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/icon48.png",
    "default_title": "拦截请求",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "<all_urls>",
    "browsingData"
  ],
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "background": {
    "scripts": ["js/forward.js", "./js/constant.js", "./js/background.js"]
  }
}
