{
  "name": "拦截请求",
  "description": "拦截chrome发出的请求，并自定义两个字段happyCookie 和 happyDomain",
  "version": "0.0.1",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "popup/images/icon48.png",
    "default_title": "拦截请求",
    "default_popup": "./popup/index.html"
  },
  "permissions": [
    "cookies",
    "webRequest",
    "storage",
    "webRequestBlocking",
    "<all_urls>",
    "browsingData"
  ],
  "icons": {
    "48": "popup/images/icon48.png",
    "128": "popup/images/icon128.png"
  },
  "background": {
    "scripts": ["background/forward.js", "background/index.js"]
  }
}
