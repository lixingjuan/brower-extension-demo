{
  "name": "Chrome插件Demo",
  "version": "1.0",
  "description": "React开发chrome插件Demo。",
  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2,
  "page_action": {
    "default_icon": "images/icon.png",
    "default_title": "React CRX",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["js/constant.js", "js/background.js"]
  },
  "permissions": [
    "cookies",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "browsingData",
    "<all_urls>"
  ],
  "web_accessible_resources": ["insert.js"]
}
